<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UI">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="59dp"
        android:layout_y="474dp"
        android:text="Favorite Drink"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="59dp"
        android:layout_y="411dp"
        android:text="Name"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/genderResultText"
        android:layout_width="244dp"
        android:layout_height="wrap_content"
        android:layout_x="104dp"
        android:layout_y="439dp" />

    <TextView
        android:id="@+id/text3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="51dp"
        android:layout_y="262dp"
        android:text="Result" />

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="202dp"
        android:layout_height="38dp"
        android:layout_x="52dp"
        android:layout_y="105dp"
        android:entries="@array/team" />

    <Spinner
        android:id="@+id/spinner3"
        android:layout_width="98dp"
        android:layout_height="38dp"
        android:layout_x="263dp"
        android:layout_y="105dp" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="308dp"
        android:layout_height="wrap_content"
        android:layout_x="53dp"
        android:layout_y="46dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name....." />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="76dp"
        android:layout_height="wrap_content"
        android:layout_x="285dp"
        android:layout_y="164dp"
        app:srcCompat="@android:drawable/btn_star_big_on" />

    <ToggleButton
        android:id="@+id/toggleButton"
        android:layout_width="74dp"
        android:layout_height="wrap_content"
        android:layout_x="286dp"
        android:layout_y="226dp"
        android:text="ToggleButton" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="84dp"
        android:layout_height="wrap_content"
        android:layout_x="52dp"
        android:layout_y="162dp"
        android:text="Pepsi" />

    <CheckBox
        android:id="@+id/checkBox4"
        android:layout_width="84dp"
        android:layout_height="wrap_content"
        android:layout_x="142dp"
        android:layout_y="162dp"
        android:text="Coke" />

    <CheckBox
        android:id="@+id/checkBox2"
        android:layout_width="84dp"
        android:layout_height="wrap_content"
        android:layout_x="52dp"
        android:layout_y="199dp"
        android:text="Fanta" />

    <RadioGroup
        android:layout_width="190dp"
        android:layout_height="wrap_content"
        android:layout_x="51dp"
        android:layout_y="290dp">

        <RadioButton
            android:id="@+id/radioButton3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Male" />

        <RadioButton
            android:id="@+id/radioButton2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Female" />

    </RadioGroup>

    <TextView
        android:id="@+id/text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="57dp"
        android:layout_y="383dp"
        android:text="Result" />

    <TextView
        android:id="@+id/nameResultText"
        android:layout_width="245dp"
        android:layout_height="wrap_content"
        android:layout_x="102dp"
        android:layout_y="410dp" />

    <TextView
        android:id="@+id/nameResultText3"
        android:layout_width="290dp"
        android:layout_height="62dp"
        android:layout_x="59dp"
        android:layout_y="494dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="59dp"
        android:layout_y="441dp"
        android:text="Gender"
        android:textSize="12sp" />

</AbsoluteLayout>

